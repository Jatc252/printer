{
    "printer": "putout",
    "plugins": [
        "printer"
    ],
    "rules": {
        "nodejs/declare": "off"
    },
    "match": {
        "{test,debug.js}": {
            "remove-console": "off"
        },
        "rules": {
            "putout/add-args": ["on", {
                "add": [
                    ["printer", "putout"]
                ]
            }]
        }
    }
}